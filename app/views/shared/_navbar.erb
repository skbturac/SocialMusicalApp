<div class="ui stackable menu">
  <div class="item">
    <%= image_tag "headphones.png" %>
  </div>
  <%= link_to "Home", root_path, class: "item" %>
  <%= link_to "Help", help_path, class: "item" %>
  <% if logged_in? %>
  <%= link_to "Profile", profile_path, class: "item" %>
  <%= link_to "POST SOMETHIN", new_post_path, class: "item" %>
  <%= link_to "Friends?", users_path, class: "item" %>
  <%= link_to 'Logout', logout_path, method: :delete, class: "item" %>
  <% else %>
  <%= link_to "Sign Up", signup_path, class: "item" %>
  <%= link_to "Log In", login_path, class: "item" %>
  <% end %>
</div>
